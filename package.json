{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npm run generate:sitemap",
    "generate:sitemap": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "commit": "npx gitmoji -c",
    "lint": "next lint --no-cache",
    "prepare": "husky install",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "lint-fix": "eslint --fix --ext .js,.jsx,.tsx,.ts .",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "check-lint": "eslint --ext .js,.jsx,.tsx,.ts .",
    "check-all": "npm run check-types && npm run check-lint && npm run build",
    "d:dev:check": "docker-compose -f ./docker/docker-compose.dev.yml --env-file .env.development config",
    "d:prod:check": "docker-compose -f ./docker/docker-compose.prod.yml --env-file .env.development config",
    "d:dev:up": "docker-compose -f ./docker/docker-compose.dev.yml --env-file .env.development up",
    "d:prod:up": "docker-compose -f ./docker/docker-compose.prod.yml --env-file .env.production up",
    "d:dev:down": "docker-compose -f ./docker/docker-compose.dev.yml --env-file .env.development down",
    "d:prod:down": "docker-compose -f ./docker/docker-compose.prod.yml --env-file .env.production down",
    "preStart": " NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' ",
    "k:m:make": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:make -x ts --env=development",
    "k:m:up": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:up --env=development",
    "k:m:down": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:down --env=development",
    "k:m:latest": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:latest --env=development",
    "k:m:rollback": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:rollback --env=development",
    "k:m:rollback:all": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:rollback --all --env=development",
    "k:s:make": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex seed:make  -x ts --env=development",
    "k:s:run": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex seed:run --env=development",
    "k:s:single": "NODE_ENV=development TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex seed:run --env=development --specific=",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "import:db": "chmod +x ./internals/scripts/init-db.sh && ./internals/scripts/init-db.sh mode=development file=visaoswal",
    "backup:db": "chmod +x ./internals/scripts/backup-db.sh && ./internals/scripts/backup-db.sh mode=development file=visaoswal"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@graphql-tools/load-files": "^6.6.0",
    "@graphql-tools/merge": "^8.3.0",
    "@graphql-tools/schema": "^8.5.0",
    "@heroicons/react": "^1.0.4",
    "apollo-server-micro": "^3.9.0",
    "axios": "^0.21.4",
    "cloudinary": "^1.29.1",
    "clsx": "^1.1.1",
    "dotenv": "^16.0.1",
    "formik": "^2.2.9",
    "graphql": "^16.5.0",
    "graphql-fields-list": "^2.2.4",
    "ioredis": "^5.0.5",
    "jet-logger": "^1.1.5",
    "knex": "^2.1.0",
    "logatim": "^0.9.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "next": "^12.0.10",
    "next-pwa": "^5.5.2",
    "next-sitemap": "^2.1.15",
    "c": "^0.0.13",
    "objection": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.39.0",
    "sharp": "^0.30.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-react": "^7.14.5",
    "@emotion/eslint-plugin": "^11.2.0",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/micro-cors": "^0.1.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.2",
    "@types/react": "17.0.21",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "autoprefixer": "^10.4.2",
    "commitlint": "^13.1.0",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.2",
    "gitmoji-cli": "^4.6.1",
    "husky": "^7.0.0",
    "postcss": "^8.4.5",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.18",
    "typescript": "4.4.3"
  }
}
